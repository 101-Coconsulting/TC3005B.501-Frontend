interface Props {
  headers: string[];
}

const { headers } = Astro.props;
---

<tr class="bg-neutral-200">
  {
    headers.map((header) => (
      <th class="px-4 py-3 font-semibold text-left text-neutral-800">
        {header}
      </th>
    ))
  }
</tr>
